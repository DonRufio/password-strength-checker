  "homepage": "https://donrufio.github.io/password-strength-checker",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gh-pages": "^6.2.0"
  },
 "scripts": {
    "start": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts start",
